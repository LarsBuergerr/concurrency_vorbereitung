class XXX {
  private int a;


  XXX(int a) {this.a = a;}

  XXX(XXX x) {this.a = x.a;}

  void setA(int a) {this.a = a;}
}

class YYY {

  ToBeImmutable tbi;

  void register(ToBeImmutable tbi) {this.tbi = tbi;}
}

class ToBeImmutable {
  private int age;
  private String name;
  private XXX xxx;

  public ToBeImmutable(int age, String name, XXX xxx, YYY yyy) {
    this.age = age;
    this.name = name;
    this.xxx = xxx;
    yyy.register(this); // problem
  }

  public int getAge() {return age;}
  public String getName() {return name;}
  public XXX getXxx() { return xxx;}

  public void setAge(int age) { this.age = age;}
}